{
    "name": "echo-bedriftstur-frontend",
    "description": "Frontend for the website of echo's student trip to Oslo.",
    "private": true,
    "license": "GPL-3.0",
    "author": "Andreas Salhus Bakseter",
    "homepage": "https://github.com/bakseter/echo-bedriftstur-frontend",
    "scripts": {
        "dev": "elm-app start",
        "build": "sh build",
        "test": "elm-test",
        "review": "elm-review",
        "fix": "elm-review --fix",
        "prepare": "husky install"
    },
    "renovate": {
        "extends": ["config:base"],
        "labels": ["dependencies :handshake:"],
        "prHourlyLimit": 0,
        "stabilityDays": 3,
        "prCreation": "not-pending"
    },
    "dependencies": {
        "elm": "0.19.1-5",
        "terser": "5.8.0"
    },
    "devDependencies": {
        "elm-review": "2.5.5",
        "elm-test": "0.19.1-revision7",
        "husky": "7.0.2"
    },
    "optionalDependencies": {
        "create-elm-app": "^5.22.0"
    }
}
